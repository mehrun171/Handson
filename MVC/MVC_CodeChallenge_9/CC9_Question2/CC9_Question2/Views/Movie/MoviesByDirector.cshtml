@model IEnumerable<CC9_Question2.Models.Movie>

@{
    ViewBag.Title = "MoviesByDirector";
}

<h2>MoviesByDirector</h2>
 
<table class="table">
<tr>
<th>Movie Name</th>
<th>Date of Release</th>
</tr>
 
    @foreach (var movie in Model)

    {
<tr>
<td>@movie.MovieName</td>
<td>@movie.DateOfRelease.ToShortDateString()</td>
</tr>

    }
</table>
 